{
	"extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
	"linterOptions": {
		"exclude": [
			"config/**/*.js",
			"node_modules/**/*.ts",
			"coverage/lcov-report/*.js"
		]
	},
	"rules": {
		"interface-over-type-literal": false,
		"jsx-no-lambda": false,
		"no-console": false,
		"no-namespace": false,
		"object-literal-sort-keys": false,
		"no-any": true
	}
}
